{
  "manifest_version": 2,
  "name": "Playback Rate Controller",
  "version": "1.0",
  "description": "Change speed of audios and videos with keyboard shortcuts.",
  "icons": {
    "32": "icons/icon-32.png",
    "64": "icons/icon-64.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["src/style.css"],
      "js": ["src/main.js"]
    }
  ]
}
